bool compare_lists(SinglyLinkedListNode* head1, SinglyLinkedListNode* head2) {
      
      SinglyLinkedListNode* t1= head1;
      SinglyLinkedListNode* t2=head2;
     int n1=0;
     int n2=0;
     while(t1!=NULL){
      t1=t1->next;
      n1++;
     }
     while(t2!=NULL){
      t2=t2->next;
      n2++;
     }
     if(n1==n2)return 1;
     else return 0;
   
}
